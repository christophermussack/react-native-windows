<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <!-- TODO: Rename as OfficeReact.Windows, Office.ReactWindows...? -->
    <id>OfficeReact.Win32</id>
    <version>$version$</version>
    <description>Contains Win32 Implementation of React-Native</description>
    <authors>Microsoft</authors>
    <projectUrl>https://office.visualstudio.com/ISS/_git/react-native</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
  </metadata>
  <files>

    <!-- Headers. Run Scripts\Tfs\Layout-Headers.ps1 -->
    <file src="inc\stubs\**\*.*"    target="inc" />
    <file src="inc\include\**\*.*"  target="inc\include" />
    <file src="inc\folly\**\*.*"    target="inc\folly" />

    <file src="inc\cxxreact\cxxmodule.h"             target="inc\cxxreact" />
    <file src="inc\cxxreact\Instance.h"              target="inc\cxxreact" />
    <file src="inc\cxxreact\jsargumenthelpers-inl.h" target="inc\cxxreact" />
    <file src="inc\cxxreact\jsargumenthelpers.h"     target="inc\cxxreact" />
    <file src="inc\cxxreact\JSBigString.h"           target="inc\cxxreact" />
    <file src="inc\cxxreact\JSCExecutor.h"           target="inc\cxxreact" />
    <file src="inc\cxxreact\JSCNativeModules.h"      target="inc\cxxreact" />
    <file src="inc\cxxreact\JSExecutor.h"            target="inc\cxxreact" />
    <file src="inc\cxxreact\MessageQueueThread.h"    target="inc\cxxreact" />
    <file src="inc\cxxreact\ModuleRegistry.h"        target="inc\cxxreact" />
    <file src="inc\cxxreact\NativeToJsBridge.h"      target="inc\cxxreact" />
    <file src="inc\cxxreact\NativeModule.h"          target="inc\cxxreact" />

    <file src="inc\yoga\Yoga\Yoga.h"                 target="inc\yoga" />
    <file src="inc\yoga\Yoga\YGMacros.h"             target="inc\yoga" />
    <file src="inc\yoga\Yoga\YGNodeList.h"           target="inc\yoga" />
    <file src="inc\yoga\Yoga\YGEnums.h"              target="inc\yoga" />

    <file src="inc\Desktop\msoFolly.h"               target="inc\msoFolly"/>

    <file src="inc\Core\DevSettings.h"               target="inc"/>
    <file src="inc\Core\INativeUIManager.h"          target="inc"/>
    <file src="inc\Core\InstanceManager.h"           target="inc"/>
    <file src="inc\Core\IReactRootView.h"            target="inc"/>
    <file src="inc\Core\Logging.h"                   target="inc"/>
    <file src="inc\Core\MemoryTracker.h"             target="inc"/>
    <file src="inc\Core\NativeModuleProvider.h"      target="inc"/>
    <file src="inc\Core\Sandbox\SandboxEndpoint.h"   target="inc"/>
    <file src="inc\Core\ShadowNode.h"                target="inc"/>
    <file src="inc\Core\Tracing.h"                   target="inc"/>
    <file src="inc\Core\Modules\I18nModule.h"        target="inc"/>

    <!-- Windows Desktop artifacts -->
    <file src="Win32\Debug\React.Windows.Desktop.DLL\react-native-win32.*"    target="lib\debug\x86" exclude="**\*.iobj;**\*.ipdb;**\*.exp;**\*.ilk" />
    <file src="Win32\Release\React.Windows.Desktop.DLL\react-native-win32.*"  target="lib\ship\x86" exclude="**\*.iobj;**\*.ipdb;**\*.exp;**\*.ilk" />
    <file src="x64\Debug\React.Windows.Desktop.DLL\react-native-win32.*"      target="lib\debug\x64" exclude="**\*.iobj;**\*.ipdb;**\*.exp;**\*.ilk" />
    <file src="x64\Release\React.Windows.Desktop.DLL\react-native-win32.*"    target="lib\ship\x64" exclude="**\*.iobj;**\*.ipdb;**\*.exp;**\*.ilk" />

    <!-- Windows Universal artifacts -->
    <file src="Win32\Debug\ReactUWP\React.UWP.*"    target="lib\debug\x86"  exclude="**\*.iobj;**\*.ipdb;**\*.exp;**\*.ilk" />
    <file src="Win32\Release\ReactUWP\React.UWP.*"  target="lib\ship\x86"   exclude="**\*.iobj;**\*.ipdb;**\*.exp;**\*.ilk" />
    <file src="x64\Debug\ReactUWP\React.UWP.*"      target="lib\debug\x64"  exclude="**\*.iobj;**\*.ipdb;**\*.exp;**\*.ilk" />
    <file src="x64\Release\ReactUWP\React.UWP.*"    target="lib\ship\x64"   exclude="**\*.iobj;**\*.ipdb;**\*.exp;**\*.ilk" />
    <file src="ARM\Debug\ReactUWP\React.UWP.*"      target="lib\debug\arm"  exclude="**\*.iobj;**\*.ipdb;**\*.exp;**\*.ilk" />
    <file src="ARM\Release\ReactUWP\React.UWP.*"    target="lib\ship\arm"   exclude="**\*.iobj;**\*.ipdb;**\*.exp;**\*.ilk" />

  </files>
